    "styles": {
        "blocks": {
            "core/buttons": {
                "elements": {
                    "button": {
                            "color": {
                                "background": "var(--wp--preset--color--blue)",
                                "text": "var(--wp--preset--color--paper)"
                            }
                    }
                }
            },
            "core/list": {
                "typography": {
                    "lineHeight": "1.3rem"
                }
            },
            "core/pullquote": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--work-sans)",
                    "fontSize": "2rem",
                    "fontWeight": "400",
                    "fontStyle": "normal"
                    },
                "color": {
                    "background": "var(--wp--preset--color--red)",
                    "text": "var(--wp--preset--color--base)"
                },
                "elements": {
                    "cite": {
                        "typography": {
                            "fontFamily": "var(--wp--preset--font-family--zilla-slab)",
                            "fontSize": "1.2rem",
                            "fontWeight": "400"
